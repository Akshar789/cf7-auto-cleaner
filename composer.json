{
  "name": "cf7-auto-cleaner/cf7-auto-cleaner",
  "description": "WordPress plugin for Contact Form 7 to automatically filter profanity and promotional content",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "autoload": {
    "classmap": [
      "includes/",
      "admin/"
    ]
  },
  "scripts": {
    "test": "phpunit",
    "phpcs": "phpcs --standard=WordPress .",
    "phpcbf": "phpcbf --standard=WordPress ."
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
